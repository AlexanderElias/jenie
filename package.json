{
	"name": "oxe",
	"version": "3.1.10",
	"description": "A mighty tiny web components framework/library",
	"main": "dist/oxe.min.js",
	"bin": "bin/index.js",
	"scripts": {
		"b": "node build.js",
		"s": "muleify serve -sw web dev",
		"w": "muleify pack -p src -wb src/index.js web/assets/oxe.js",
		"bd": "rm -r docs/* && muleify pack web docs",
		"ba": "npm run b && npm run bd"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AlexanderElias/oxe.git"
	},
	"author": "Alexander Elias",
	"license": "MPL-2.0",
	"bugs": {
		"url": "https://github.com/AlexanderElias/oxe/issues"
	},
	"homepage": "https://github.com/AlexanderElias/oxe#readme",
	"keywords": [
		"web",
		"component",
		"components",
		"web-components",
		"js",
		"html",
		"css",
		"framework",
		"data-binding",
		"data",
		"binding",
		"routing",
		"router",
		"http",
		"front-end",
		"x-tag",
		"polymer",
		"jenie",
		"oxe"
	],
	"directories": {
		"docs": "docs",
		"dist": "dist",
		"src": "src"
	},
	"devDependencies": {
		"muleify": "^4.0.3"
	},
	"dependencies": {
		"cliy": "^1.1.2"
	}
}
