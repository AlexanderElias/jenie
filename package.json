{
  "name": "jenie",
  "version": "1.4.11",
  "description": "Light weight powerful web components framework",
  "main": "dist/jenie.min.js",
  "scripts": {
    "sd": "muleify serve -s docs",
    "wj": "muleify pack -p src -w src/jenie.b.js docs/assets/jenie.js",
    "wr": "muleify pack -w docs/views/v-root.e.js docs/views/v-root.js",
    "bd": "muleify pack src/jenie.b.js dist/jenie.js",
    "bm": "muleify pack -m src/jenie.b.js dist/jenie.min.js",
    "bp": "cat dist/webcomponents-lite.min.js dist/jenie.min.js > dist/jenie.polly.min.js",
    "ba": "npm run bd && npm run bm && npm run bp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderElias/jenie.git"
  },
  "author": "Alexander Elias",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/AlexanderElias/jenie/issues"
  },
  "homepage": "https://github.com/AlexanderElias/jenie#readme",
  "keywords": [
    "web",
    "components",
    "web-components",
    "js",
    "html",
    "css",
    "framework",
    "data-binding",
    "data",
    "binding",
    "routing",
    "router",
    "front-end"
  ],
  "directories": {
    "doc": "docs"
  },
  "dependencies": {},
  "devDependencies": {
    "node-es-module-loader": "^0.3.8"
  }
}
