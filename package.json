{
	"name": "oxe",
	"version": "1.10.7",
	"description": "A mighty tinny web components framework/library",
	"main": "dist/oxe.min.js",
	"scripts": {
		"serve": "muleify serve -sw web dev",
		"watch": "muleify pack -p src -wb src/index.js web/assets/oxe.js",
		"build-docs": "rm -r docs/* && muleify pack web docs",
		"build-scripts": "node build.js",
		"build-all": "npm run build-scripts && npm run build-docs"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AlexanderElias/oxe.git"
	},
	"author": "Alexander Elias",
	"license": "MPL-2.0",
	"bugs": {
		"url": "https://github.com/AlexanderElias/oxe/issues"
	},
	"homepage": "https://github.com/AlexanderElias/oxe#readme",
	"keywords": [
		"web",
		"component",
		"components",
		"web-components",
		"js",
		"html",
		"css",
		"framework",
		"data-binding",
		"data",
		"binding",
		"routing",
		"router",
		"http",
		"front-end",
		"x-tag",
		"polymer",
		"jenie",
		"oxe"
	],
	"directories": {
		"doc": "docs"
	},
	"devDependencies": {
		"muleify": "^2.7.10"
	}
}
