{
  "name": "jenie",
  "version": "1.1.3",
  "description": "Light weight powerful web components framework",
  "main": "src/jenie.js",
  "scripts": {
	"serve": "muleify serve -s test",
	"watch": "muleify pack -p src -w src/jenie.b.js test/jenie.js",
	"build-dev": "muleify pack src/jenie.b.js dist/jenie.js",
    "build-min": "muleify pack -m src/jenie.b.js dist/jenie.min.js",
	"build-pol": "cat dist/webcomponents-lite.min.js dist/jenie.min.js > dist/jenie.polly.min.js",
	"build-all": "npm run build-dev && npm run build-min && npm run build-pol"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AlexanderElias/jenie.git"
  },
  "author": "Alexander Elias",
  "license": "MPL-2.0",
  "bugs": {
    "url": "https://github.com/AlexanderElias/jenie/issues"
  },
  "homepage": "https://github.com/AlexanderElias/jenie#readme"
}
