{
	"name": "jenie",
	"version": "1.7.1",
	"description": "A mighty tinny web components framework/library",
	"main": "dist/jenie.min.js",
	"scripts": {
		"build-docs": "rm -r docs/* && muleify pack web docs",
		"serve": "muleify serve -sw web dev",
		"watch": "muleify pack -p src -wb src/jenie.js web/assets/jenie.js",
		"bd": "muleify pack -b src/jenie.js dist/jenie.js",
		"bm": "muleify pack -bm src/jenie.js dist/jenie.min.js",
		"bp": "cat dist/webcomponents-lite.min.js dist/jenie.min.js > dist/jenie.polly.min.js",
		"ba": "npm run bd && npm run bm && npm run bp"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/AlexanderElias/jenie.git"
	},
	"author": "Alexander Elias",
	"license": "MPL-2.0",
	"bugs": {
		"url": "https://github.com/AlexanderElias/jenie/issues"
	},
	"homepage": "https://github.com/AlexanderElias/jenie#readme",
	"keywords": [
		"web",
		"component",
		"components",
		"web-components",
		"js",
		"html",
		"css",
		"framework",
		"data-binding",
		"data",
		"binding",
		"routing",
		"router",
		"http",
		"front-end"
	],
	"directories": {
		"doc": "docs"
	},
	"dependencies": {},
	"devDependencies": {
		"node-es-module-loader": "^0.3.8"
	}
}
