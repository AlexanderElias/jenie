{
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"globals": {
		"Prism": true,
		"Jenie": false
	},
	"parserOptions": {
		"sourceType": "module"
	},
	"rules": {
		"strict": 0,
		"no-undef": 1,
		"no-console": 0,
		"no-extra-semi": 1,
		"no-unused-vars": 1,
		"no-cond-assign": 0,
		"indent": [1, "tab", { "SwitchCase": 1 }],
		"semi": [1, "always"],
		"quotes": [1, "single", "avoid-escape"],
		"comma-spacing": [1, { "before": false, "after": true }]
	}
}
